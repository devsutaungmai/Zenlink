{
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "regions": ["fra1"],
  "env": {
    "JWT_SECRET": "@jwt-secret"
  },
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "true"
    }
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
